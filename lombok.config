#
# lombok configuration
#

lombok.addLombokGeneratedAnnotation = true
# lombok.addGeneratedAnnotation = false
# lombok.addJavaxGeneratedAnnotation = false

# due to the change in Lombok 1.16.20 it has to be explicitly set to false to make Lombok add `@ConstructorProperties` annotation.
lombok.anyConstructor.suppressConstructorProperties = false

# use Objects.requireNonNull() to implement @NonNull
lombok.nonNull.exceptionType=jdk

